<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/" xmlns:moz="http://www.mozilla.org/2006/browser/search/">
	<ShortName></ShortName>
	<Description></Description>
	<InputEncoding>UTF-8</InputEncoding>
	<Image width="16" height="16" type="image/x-icon"></Image>
	<Url method="get" template="IPADDRESS" type="text/html">
		<Param name="search" value="{searchTerms}"></Param>
		<Param name="direct" value="true"></Param>
	</Url>
	<moz:SearchForm>IPADDRESS</moz:SearchForm>
</OpenSearchDescription>
